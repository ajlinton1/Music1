@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Player</title>

    <link href='@Url.Content("~/Content/Music.css")' rel="stylesheet" type="text/css" />

    <script src='@Url.Content("~/Scripts/jquery-1.7.1.js")'  type="text/javascript" ></script>
    <script src='@Url.Content("~/Scripts/knockout-2.1.0.js")'  type="text/javascript" ></script>
    <script src='@Url.Content("~/Scripts/knockout.mapping-latest.js")'  type="text/javascript" ></script>

</head>
<body>

    <table data-bind="foreach: songs" border='1' cellspacing='1' cellpadding='1' id='songTable'>
        <tr>
            <td ><a href="#" class="songTitle" data-bind="    text: TITLE"></a></td>
            <td data-bind="text: ARTIST"></td>
            <td data-bind="text: GENRE"></td>
            <td data-bind="text: RATING"></td>
            <td data-bind="text: LOCATION"></td>
        </tr>
    </table>

    <script src='@Url.Content("~/Scripts/SongList.js")' type="text/javascript" ></script>

</body>
</html>

